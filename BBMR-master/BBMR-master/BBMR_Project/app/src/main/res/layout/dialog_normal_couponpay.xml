            android:layout_height="85dp"
            android:layout_marginStart="12dp"
            android:backgroundTint="#73A4C8"
            android:insetTop="0dp"
            android:insetBottom="0dp"
            android:text="4"
            android:textColor="#000000"
            android:textSize="28sp"
            android:textStyle="bold"
            app:cornerRadius="16dp"
            app:layout_constraintBottom_toBottomOf="@+id/tvBckGnd"
            app:layout_constraintStart_toEndOf="@+id/btn3N"
            app:layout_constraintTop_toTopOf="@+id/tvBckGnd"
            app:layout_constraintVertical_bias="0.104" />

        <Button
            android:id="@+id/btn5N"
            android:layout_width="85dp"
            android:layout_height="85dp"
            android:layout_marginStart="12dp"
            android:backgroundTint="#73A4C8"
            android:insetTop="0dp"
            android:insetBottom="0dp"
            android:text="5"
            android:textColor="#000000"
            android:textSize="28sp"
            android:textStyle="bold"
            app:cornerRadius="16dp"
            app:layout_constraintBottom_toBottomOf="@+id/tvBckGnd"
            app:layout_constraintStart_toEndOf="@+id/btn4N"
