            app:cornerRadius="16dp"
            app:layout_constraintBottom_toBottomOf="@+id/tvBckGnd"
            app:layout_constraintEnd_toEndOf="@+id/tvBckGnd"
            app:layout_constraintStart_toEndOf="@+id/btn0N"
            app:layout_constraintTop_toBottomOf="@+id/btnBckSpceDNC"
