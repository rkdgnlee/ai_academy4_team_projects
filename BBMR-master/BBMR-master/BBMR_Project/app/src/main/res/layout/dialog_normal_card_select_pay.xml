<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Dialog.Normal_CardSelectPayDialog">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="100dp"
        android:background="@drawable/normal_dialog_background"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.575"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.405">

        <TextView
            android:id="@+id/textView22"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="56dp"
            android:text="사용 가능한 모바일 페이를 선택해주세요"
            android:textColor="@color/black"
            android:textSize="28sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView27"
            android:layout_width="550dp"
            android:layout_height="4dp"
            android:layout_marginTop="28dp"
            android:background="@color/mainColor"
            android:backgroundTintMode="add"
            android:text="TextView"
            app:layout_constraintEnd_toEndOf="@+id/textView22"
            app:layout_constraintHorizontal_bias="0.562"
            app:layout_constraintStart_toStartOf="@+id/textView22"
            app:layout_constraintTop_toBottomOf="@+id/textView22" />

        <android.widget.Button
            android:id="@+id/btn1DNCSP"
            android:layout_width="200dp"
            android:layout_height="150dp"
            android:layout_marginStart="44dp"
            android:layout_marginTop="188dp"
            android:background="@drawable/normal_pay_background"
            android:backgroundTint="@color/white"
            android:drawableTop="@drawable/paylogo_apple"
            android:gravity="bottom|center_horizontal"
            android:insetTop="0dp"
            android:insetBottom="0dp"
            android:paddingTop="20dp"
            android:paddingBottom="4dp"
            android:text="애플페이"
            android:textColor="@color/mainColor"
            android:textSize="20sp"
            android:textStyle="bold"
            app:cornerRadius="16dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <android.widget.Button
            android:id="@+id/btn2DNCSP"
            android:layout_width="200dp"
            android:layout_height="150dp"
            android:layout_marginTop="188dp"
            android:background="@drawable/normal_pay_background"
            android:backgroundTint="@color/white"
            android:drawableTop="@drawable/paylogo_naverpay"
            android:gravity="bottom|center_horizontal"
            android:insetTop="0dp"
            android:insetBottom="0dp"
            android:paddingTop="30dp"
            android:paddingBottom="4dp"
